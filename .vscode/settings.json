{
	"git.ignoreLimitWarning": true,
	"path-intellisense.mappings": {
		"views": "${workspaceRoot}/src/views",
		"components": "${workspaceRoot}/src/components"
    },
	"search.exclude": {
		"**/node_modules": true,
		"**/dist": true,
	},
	"files.exclude": {
		"**/.git": true,
	},
	"prettier.tabWidth": 4,
	"prettier.useTabs": true,
	"prettier.printWidth": 120
}
